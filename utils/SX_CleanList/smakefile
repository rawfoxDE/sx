.c.o:
		SC $(CFLAGS) $(CPU) $*.c

CFLAGS		= NMINC MCCONS STREQ STRMERGE NOSTKCHK DEFINE=__PROTO__ PARAMETERS=REGISTERS OPT
LFLAGS		= SC SD NOICONS STRIPDEBUG

CPU      = CPU=000
OBJS		= SX_CleanList.o
LIBS		= LIB:sc.lib LIB:amiga.lib

SX_CleanList:	$(OBJS)
		SLINK LIB:c.o $(OBJS) TO $@ LIB $(LIBS) $(LFLAGS)
